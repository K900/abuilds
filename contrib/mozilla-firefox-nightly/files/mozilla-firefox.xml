<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE default-apps SYSTEM "gnome-da-list.dtd">
<default-apps>
  <web-browsers>
    <web-browser>
      <name>Mozilla Firefox</name>
      <executable>/usr/bin/@FX@</executable>
      <command>/usr/bin/@FX@ %s</command>
      <icon-name>@FX@</icon-name>
      <run-in-terminal>false</run-in-terminal>
      <netscape-remote>true</netscape-remote>
      <tab-command>/usr/bin/@FX@ -new-tab "%s"</tab-command>
      <win-command>/usr/bin/@FX@ -new-window "%s"</win-command>
    </web-browser>
  </web-browsers>
</default-apps>
