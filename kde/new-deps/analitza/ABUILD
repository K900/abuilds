# ABUILD by K900
pkgname=analitza
pkgver=4.7.90
pkgbuild=1
arch=('auto')
shortdesc="Math library for KAlgebra"
tags=('libs sci-mathematics')
source=("ftp://ftp.kde.org/pub/kde/unstable/${pkgver}/src/${pkgname}-${pkgver}.tar.bz2")



